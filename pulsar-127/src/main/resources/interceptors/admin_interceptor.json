{
  "configMap" : {
    "/admin/v2/namespaces/{tenant}/{namespace}/backlogQuota" : {
      "POST" : {
        "conditions" : {
          "limitSize" : "^[1-9]\\d{0,8}?$"
        },
        "errorCode" : 412,
        "tip" : "The backlog must lower than 1000000000"
      }
    },
    "/admin/v2/namespaces/{tenant}/{namespace}/retention" : {
      "POST" : {
        "conditions" : {
          "retentionSizeInMB" : "^[1-9]\\d{0,2}?$"
        },
        "errorCode" : 412,
        "tip" : "The retention size must lower than 1000MB"
      }
    },
    "/admin/v2/persistent/{tenant}/{namespace}/{topic}/partitions" : {
      "PUT" : {
        "conditions" : {
          "partitions" : "^(0|[1-9]|[1][0-6])$"
        },
        "tip" : "Allowed max partitions are 16",
        "errorCode" : 412
      },
      "POST" : {
        "conditions" : {
          "partitions" : "^(0|[1-9]|[1][0-6])$"
        },
        "tip" : "Allowed max partitions are 16",
        "errorCode" : 412
      }
    },
